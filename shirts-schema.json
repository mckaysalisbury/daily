{
    "$schema": "http://json-schema.org/schema",
    "type": "object",
    "properties": {
      "data": {
        "type": "array",
        "items": {
            "type": "object",
            "required": ["id", "name", "text", "images", "keywords"],
            "properties": {
                "id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "style": {
                    "type": "string",
                    "enum": ["t-shirt", "polo", "pocket", "long sleeve", "button down"]
                },
                "text": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "images-name": {
                    "type":"string"
                },
                "images": {
                    "type": "array",
                    "items": {
                        "type":"string",
                        "enum": ["front", "back", "logo", "left", "right", "dark"]
                    }
                },
                "keywords": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "source": {
                    "type": "string"
                },
                "dates": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "description": "a Shirt",
            "markdownDescription": "a Shirt"
        }
      }
    }
  }
